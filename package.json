{
  "name": "telegram-bot-example",
  "version": "1.0.0",
  "description": "Example of a telegram bot with Telegraf",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:fix": "npx eslint \"**/*.{js,jsx}\" --fix",
    "knex:make": "npx knex --knexfile knexFile.js migrate:make",
    "knex:migrate": "npx knex --knexfile knexFile.js migrate:latest",
    "knex:rollback": "npx knex --knexfile knexFile.js migrate:rollback"
  },
  "keywords": [
    "telegram-bot",
    "telegraf"
  ],
  "author": "SatsCzar",
  "license": "Unlicense",
  "dependencies": {
    "@herbsjs/herbarium": "^1.5.0",
    "@herbsjs/herbs": "^2.0.0",
    "@herbsjs/herbs2knex": "^2.1.1",
    "@satsczar/telegraf-command-parts": "^1.0.4",
    "cron": "^2.3.1",
    "cron-validator": "^1.3.1",
    "cronstrue": "^2.28.0",
    "dotenv": "^16.3.1",
    "lightning": "^9.11.3",
    "sqlite": "^5.0.1",
    "sqlite3": "^5.1.6",
    "telegraf": "^4.12.2"
  },
  "devDependencies": {
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0"
  }
}
